{
  "name": "vacproj",
  "version": "1.0.0",
  "description": "Solita 2021",
  "main": "index.js",
  "scripts": {
    "setup": "npm install && npm run populdb && npm run build && npm run test",
    "start": "node ./server/index.js",
    "populdb": "node ./server/setup.js",
    "client": "webpack serve --mode=development --devtool inline-source-map --hot",
    "server": "nodemon --signal SIGHUP ./server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "webpack --mode=production",
    "test": "cross-env NODE_ENV=test jest --verbose --silent"
  },
  "author": "Jonne Lehtonen",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.13.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^0.19.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-datetime-picker": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-minimal-pie-chart": "^8.2.0",
    "style-loader": "^3.0.0",
    "supertest": "^6.1.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
